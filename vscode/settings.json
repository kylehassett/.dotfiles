{
    "breadcrumbs.enabled": true,
    "cSpell.language": "en",
    "cSpell.userWords": [
        "appspot",
        "copyright",
        "daos",
        "emotionals",
        "firebaseapp",
        "fontawesome",
        "formik",
        "gealio",
        "hassett",
        "keyframes",
        "Kyphotic",
        "logicals",
        "musiqu",
        "musique",
        "ohpin",
        "semibold",
        "signup",
        "sqlite",
        "strftime",
        "Unmount",
        "werkzeug"
    ],
    "editor.autoClosingBrackets": "never",
    "editor.dragAndDrop": false,
    "editor.fontFamily": "Fira Code",
    "editor.fontLigatures": true,
    "editor.fontSize": 13,
    "editor.minimap.enabled": false,
    "editor.rulers": [100],
    "editor.suggestSelection": "first",
    "emmet.excludeLanguages": [],
    "emmet.includeLanguages": {
        "javascript": "javascriptreact"
    },
    "fileheader.Author": "Kyle R Hassett",
    "fileheader.LastModifiedBy": "Kyle R Hassett",
    "fileheader.tpl": "/*\r\n * @Author: {author} \r\n * @Date: {createTime} \r\n * @Last Modified by:   {lastModifiedBy} \r\n * @Last Modified time: {updateTime} \r\n * @Copyright: 2018 Kyle R Hassett \r\n */\r\n",
    "files.autoSave": "onFocusChange",
    "files.insertFinalNewline": true,
    "files.exclude": {
        "**/.git": false
    },
    "files.trimFinalNewlines": true,
    "files.trimTrailingWhitespace": true,
    "git.autofetch": true,
    "indentRainbow.colors": [
        "rgba(24, 78, 119,0.15)",
        "rgba(30, 96, 145,0.15)",
        "rgba(26, 117, 159, 0.15)",
        "rgba(22, 138, 173,0.15)",
        "rgba(52, 160, 164,0.15)",
        "rgba(82, 182, 154,0.15)",
        "rgba(82, 182, 154,0.1)",
        "rgba(82, 182, 154,0.05)",
        "rgba(82, 182, 154,0.025)"
    ],
    "indentRainbow.errorColor": "rgba(128,32,32,0.05)",
    "python.jediEnabled": false,
    "python.languageServer": "Default",
    "terminal.integrated.rendererType": "dom",
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "workbench.colorTheme": "Dracula",
    "workbench.editorAssociations": {
        "*.ipynb": "jupyter.notebook.ipynb"
    },
    "workbench.iconTheme": "material-icon-theme",
    "[javascript]": {
        "editor.tabSize": 4
    },
    "[python]": {
        "editor.tabSize": 4
    },
    "editor.cursorSmoothCaretAnimation": true,
    "window.zoomLevel": -1,
    "vsicons.dontShowNewVersionMessage": true,
    "svelte.enable-ts-plugin": true,
    "terminal.integrated.tabs.description": "${task}",
    "terminal.integrated.tabs.title": "${cwdFolder}",
    "terminal.integrated.profiles.osx": {
        "fish": {
            "path": "/usr/local/bin/fish", // overriding
            "args": [
                "-l"
            ]
        }
    },
    "editor.formatOnSave": true,
}
